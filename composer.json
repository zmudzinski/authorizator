{
    "name": "tzm/authorizator",
    "description": "This is an easy to use, powerful and extendable Laravel package for authorization user actions via custom channels.",
    "type": "library",
    "keywords": ["laravel", "authorizator", "2fa", "verification", "authorization"],
    "homepage": "https://github.com/zmudzinski/authorizator",
    "require": {
        "php": "^7.2",
        "guzzlehttp/guzzle": "~6.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Tomek Żmudziński",
            "email": "t.zmudzinski@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Tzm\\Authorizator\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tzm\\Authorizator\\AuthorizatorProvider"
            ]
        }
    }
}
